{
  "name": "yanzi",
  "version": "1.0.1",
  "author": "liangyanxiang<liangyanxiang@juzix.io>",
  "description": "燕子客户端",
  "license": null,
  "main": "./dist/electron/main.js",
  "scripts": {
    "build": "node build/build.js && electron-builder",
    "build:dir": "node build/build.js && electron-builder --dir",
    "build:clean": "cross-env BUILD_TARGET=clean node build/build.js",
    "build:web": "cross-env BUILD_TARGET=web node build/build.js",
    "dev": "node build/dev-runner.js",
    "pack": "npm run pack:main && npm run pack:renderer",
    "pack:main": "cross-env NODE_ENV=production webpack --progress --colors --config build/webpack.main.config.js",
    "pack:renderer": "cross-env NODE_ENV=production webpack --progress --colors --config build/webpack.renderer.config.js",
    "postinstall": ""
  },
  "build": {
    "productName": "燕子",
    "appId": "org.liangyanxiang.yanzi",
    "directories": {
      "output": "dist"
    },
    "files": [
      "dist/electron/**/*"
    ],
    "extraFiles": [
      {
        "from": "src/main/static/",
        "to": "static",
        "filter": []
      }
    ],
    "dmg": {
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ]
    },
    "mac": {
      "icon": "build/icons/icon.icns"
    },
    "win": {
      "icon": "build/icons/icon.ico"
    },
    "linux": {
      "icon": "build/icons"
    }
  },
  "dependencies": {
    "axios": "^0.18.0",
    "crypto-js": "^3.1.9-1",
    "electron-builder-squirrel-windows": "^20.6.0",
    "moment": "^2.21.0",
    "nodemailer": "^4.6.3",
    "vue": "^2.5.16",
    "vue-electron": "^1.0.6",
    "vue-router": "^3.0.1",
    "ws": "^5.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-preset-env": "^1.3.3",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.2.0",
    "babili-webpack-plugin": "^0.1.1",
    "cfonts": "^1.1.3",
    "chalk": "^1.1.3",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "del": "^2.2.1",
    "devtron": "^1.4.0",
    "electron": "^1.8.4",
    "electron-builder": "^19.56.2",
    "electron-debug": "^1.4.0",
    "electron-devtools-installer": "^2.0.1",
    "extract-text-webpack-plugin": "^2.0.0-beta.4",
    "file-loader": "^0.11.1",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "multispinner": "^0.2.1",
    "style-loader": "^0.18.1",
    "url-loader": "^0.5.7",
    "vue-html-loader": "^1.2.4",
    "vue-loader": "^12.2.2",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.5.15",
    "webpack": "^4.1.1",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "^2.21.2"
  },
  "optionalDependencies": {
    "bufferutil": "^3.0.3",
    "utf-8-validate": "^4.0.0"
  }
}
